--init
game_states ={
	menu = 0,
	game = 1,
	over = 2,
}

function _init()
	--globals
	gravity = 0.5
	friction = 0.5
	state = 1

	objs = {}
	player = make_gameobj("tank",1,10,0,false,0,0,1,1)
	player[#player+1] = 0
	player.wrap = true
	
	player2 = make_gameobj("tank",3,40,0,false,0,0,1,1)
	player2[#player2+1] = 1
	player2.wrap = true
end

