function update_game()

	
	for o in all(objs) do
		o:update()
		
		if o!=player then
			collide_obj(o,player)
		end
		
		if not o!=player2 then
			collide_obj(o,player2)
		end
	end
	player_input(player)
	player_input(player2)
end

