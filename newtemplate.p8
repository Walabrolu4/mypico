function menu_init()
	music(menu_music)
end

function game_init()
	music(game_music)
	add_player(0,10,10,0,0,1,1)
	add_player(1,20,10,0,0,1,1)
end

function over_init()
	music(menu_music)
end

function re_init()
	if not inited then
		if state == game_states.menu then   
  	menu_init()
 	elseif state == game_states.game then
  	game_init() 
	 elseif state == game_states.over then
  	over_init()
 	end
 	inited = true
	end
end

