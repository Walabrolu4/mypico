--menu
function update_menu()
	if btn(❎) then
		state = 1
		inited = false
	end
	if btnp(🅾️,0) or btnp(🅾️,1)then
		if can_quack then 
			sfx(fx_quack)
			can_quack = false
		end
	end
	if not btn(🅾️) then
		 can_quack = true
	end
end

--game
function update_game()
	for p in all(players) do
		plr_input(p)
		plr_update(p)
	end
end

