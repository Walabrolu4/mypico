--collisions

function collide_map(obj,col_dir,flag)
	--obj == table needs x,y,w,h
	--aim = left,right,up,down
	
	local x=obj.x local y=obj.y
	local w=obj.w local h=obj.h
	local x1=0 local x2=0
	local y1=0 local y2=0
	
	if col_dir=="left" then
	   x1=x-1   y1=y
	   x2=x     y2=y+h-1
	elseif col_dir=="right" then
	   x1=x+w-1 y1=y
	   x2=x+w   y2=y+h-1
	elseif col_dir=="up" then
	   x1=x+2   y1=y-1
	   x2=x+w-3 y2=y
	elseif col_dir=="down" then
	   x1=x+2   y1=y+h
	   x2=x+w-3 y2=y+h
	end
	
	--pixels to tiles
	x1/=8	x2/=8
	y1/=8 y2/=8
	
	if fget(mget(x1,y1),flag)
	or fget(mget(x1,y2),flag)
	or fget(mget(x2,y1),flag)
	or fget(mget(x2,y2),flag) then
		return true
	else
		return false
	end		
end

function collide_obj(obj,other)
	local x1=obj.x local y1=obj.y
	local x2=other.x local y2=other.y 
	local h=1 local w=1
 
 --jeez
	if x1 > x2 + w or x1 + w < x2 or y1 > y2+h or y1+h < y2 then
		return false
	else
		return true
	end 
end

