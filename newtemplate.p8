function plr_update(p)
--update ojbects physics
		p.dx *= friction
				
		p.dy += gravity * p.grav_mul
		if p.flapping then
			p.dy = 0.5
		end
		
		p.dx = limit_speed(p.dx,p.max_dx)
		p.dy = limit_speed(p.dy,p.max_dy)
		
		
		--grav check
		if p.dy > 0 then
			p.falling = true
			if collide_map(p,"down",0) then
				p.dy=0
				p.y-=((p.y+8+1)%8)-1
			end
		end
		
	 --head check
		if p.dy<=0 then
			p.falling = false
			if collide_map(p,"up",1) then
				p.dy=0
			end
		end
		
		--l && r check
		if p.dx>0 then --l
			if collide_map(p,"right",1) then
				p.dx=0
			end	
		end
		
		if p.dx<0 then
			if collide_map(p,"left",1) then
				p.dx=0
			end
		end
				
		p.x += p.dx
		p.y += p.dy
		
		--wrap
		if wrap then
				if p.x < 0 then
					p.x = 125
				elseif p.x  > 125 then
					p.x = 0
				end
				if p.y < 0 then
					p.y = 125
				elseif p.y > 125 then
					p.y = 0
				end			
		end	
end


function plr_draw(p)
	spr(p.sp, p.x, p.y,p.w/8,p.h/8,p.flp)
end

