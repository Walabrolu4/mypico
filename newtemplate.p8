--gameobjet
function make_gameobj(_name,_sp,_x,_y,_flp,_dx,_dy,_w,_h)
	return add(objs,{
	name=_name,
	sp=_sp,
	x=_x,
	y=_y,
	w=_w or 1,
	h=_h or 1,
	flp = _flp or false,
	dx = _dx or 0,
	dy = _dy or 0,
	max_dx = 3,
	max_dy = 6,
	frm =0,
	wrap = false,
	solid = true,
	draw = function(s)
		--draw the object at its cur loc and flp
		spr(s.sp,s.x,s.y,s.w,s.h,s.flp)
	end,
	
	update = function(s)
			--update ojbects physics
			s.dx *= friction
			s.dy += gravity
			
			s.dx = limit_speed(s.dx,s.max_dx)
			s.dy = limit_speed(s.dy,s.max_dy)
			
			--grav check
			if s.dy > 0 then
				if collide_map(s,"down",0) then
					s.dy=0
					s.y-=((s.y+8+1)%8)-1
				end
			end
			
		 --head check
			if s.dy<0 then
				if collide_map(s,"up",1) then
					s.dy=0
				end
			end
			
			--l && r check
			if s.dx>0 then --l
				if collide_map(s,"right",1) then
					s.dx=0
				end	
			end
			
			if s.dx<0 then
				if collide_map(s,"left",1) then
					s.dx=0
				end
			end
					
			s.x += s.dx
			s.y += s.dy
			
			--wrap
			if s.wrap then
					if s.x < 0 then
						s.x = 125
					elseif s.x  > 125 then
						s.x = 0
					end
					if s.y < 0 then
						s.y = 125
					elseif s.y > 125 then
						s.y = 0
					end			
			end	
	end,
	})
end

