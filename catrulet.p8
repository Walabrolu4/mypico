--person
function add_person(_x,_y)
	add(persons,{
		sp=1,
		x =_x,
		y=_y,
		w=3,
		h=4,
		state = 1,
	})
end

function person_update(p)
	if collide_obj(p,kitty) then
		p.sp = 5
	else
		p.sp = 1
	end
end

function ouch(p)
	p.sp = 9
end

function person_draw(p)
	spr(p.sp,p.x,p.y,p.w,p.h)
end
