--draw

function _draw()
	cls(15)
	map(0,0)
	france=false
	for p in all(persons) do
		person_draw(p)
		if france == false then	france = collide_obj(p,kitty) end	
	end
	
	kitty_draw(kitty)
	print(rndr)

	print(tostr(france))
end
