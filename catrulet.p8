--init
rndr=0;

game_states ={
	pick = 1,
	deciding = 2,
	wl = 3,
}
p_states = {
	chillin = 1,
	loved = 2,
	bite = 3
}

function _init()
	state = 2;
	persons={}
	palt(0,false)
	palt(11,true)
	add_person(15,125-37)
	add_person(50,125-37)
	add_person(85,125-37)
end

--[[summary--
	this is a game of chance.
	
	there are
		- 3 people
		- 1 simeon
		
	pick the one you think will 
	be bit. simeon will walk aroun
	and rub,rub,rub..randomly bite
	if yourpick==bitten then
		you win
	else
		shit
	end
]]
