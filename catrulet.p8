--utils
	--colides centers
function collide_obj(obj,other)
  x1=obj.x y1=obj.y
  w1=obj.w h1=obj.h
  x2=other.x y2=other.y
  w2=other.w h2=other.h
  
  hit=false
  local xd=abs((x1+(w1/2))-(x2+(w2/2)))
  local xs=w1*0.5+w2*0.5
  local yd=abs((y1+(h1/2))-(y2+(h2/2)))
  local ys=h1/2+h2/2
  if xd<xs and 
     yd<ys then 
    hit=true 
  end
  
  return hit
end
