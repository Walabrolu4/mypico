--kitty
--get the stupid cat to collide once...
kitty={	
	sp=64,
	x=0,
	y=125-36,
	w=4,
	h=4,
	flp=false,
	state=1,
	tick=0,
	frame=1,
	step=4,
	collided=false,
}
	
cat_states={
idle = 0,
walking = 1,
rubble = 2,
bite = 3,
}
	
kitty_walk={64,68,72}
kitty_bite={136,140}
kitty_rubble={76,128,132}
function kitty_update(p)
	if state == game_states.deciding then
		collided = false
		for person in all(persons) do
			if collide_obj(kitty,person) and collided==false then
				collided=true
				i = rnd(100)
				rndr=i
				if i<99 then
					p.state = cat_states.rubble
				else
					p.state = cat_states.bite
					state = game_states.wl
				end
				break
			end
			if collided ==false then
			 p.state = cat_states.walking
			end
		end
	end
	
	--walking or rubble
	if p.state == cat_states.walking or  p.state == cat_states.rubble then
		if p.x<0 then 
			p.flp = true
		elseif p.x+8*p.w > 128 then
			p.flp=false
		end
		
		if p.flp then
			p.x +=1
		else
			p.x -=1
		end
	end
	
	--bite
	
end

function kitty_animate(p)

	--walking
	if p.state == cat_states.walking then
		p.tick = (p.tick+1)%p.step --tick goes from 1 to step
		--when tick resets to 0 
		--frame up![0 - #anim_array]
		if p.tick==0 then p.frame=p.frame%#kitty_walk+1 end
		p.sp=kitty_walk[p.frame]		
	end
	
	--rubble
	if p.state == cat_states.rubble then
		p.tick = (p.tick+1)%p.step --tick goes from 1 to step
		--when tick resets to 0 
		--frame up![0 - #anim_array]
		if p.tick==0 then p.frame=p.frame%#kitty_rubble+1 end
		p.sp=kitty_rubble[p.frame]		
	end
	
	--bite
	if p.state == cat_states.bite then
		p.tick = (p.tick+1)%p.step --tick goes from 1 to step
		--when tick resets to 0 
		--frame up![0 - #anim_array]
		if p.tick==0 then p.frame=p.frame%#kitty_bite+1 end
		p.sp=kitty_bite[p.frame]		
	end
end

function kitty_draw(p)
		spr(p.sp,p.x,p.y,p.w,p.h,p.flp)
end
