--init
function _init()
	--global shit
	gun_offset=4
	timing = 0.25

	--gun_definitions
	no_gun = -1
	shot_gun = 64
	net_gun = 65

	
	--player
	plr ={
		x=0,
		y=127-(31),
		sprite = 1,
		gun_spr = shot_gun,
		flipped=true,
	}
end



-->8
--draw
function _draw()
    cls()
				map(0,0) 
    spr(plr.sprite, plr.x, plr.y)
    spr(plr.gun_spr,plr.x+gun_offset,plr.y)
				
    print(plr.sprite)
end
-->8
--update
function _update()
    --animate
    plr.sprite += timing
    if plr.sprite > 5 then plr.sprite = 1 end

    --move
    plr.x += 1
    if plr.x > 127 then plr.x = -8 end
end
