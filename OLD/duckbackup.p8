--player
function player_update()
	--physics
	plr.dy+=gravity
	plr.dx*=friction
	
 --move
 if btn(‚¨ÖÔ∏è) then 
		plr.dx-=plr.acc
		plr.running=true
 	plr.flp = true
 	if collide_map(plr,"left",2) then plr.dx=0 plr.running=false end 
 elseif btn(‚û°Ô∏è) then
		plr.dx+=plr.acc
		plr.running=true
 	plr.flp = false 
 	if collide_map(plr,"right",2) then plr.dx=0 plr.running=false end
 end
 
 if not btn(‚¨ÖÔ∏è) and not btn (‚û°Ô∏è) then plr.running = false end
 
 --jump
 if btnp(‚¨ÜÔ∏è) and plr.landed then
 plr.dy-=plr.boost
 plr.landed=false
 sfx(1)
 end
 
 --quack
 if btn(üÖæÔ∏è) then 
 	plr.quacking = true
 	if not quackplayed then sfx(0) end
 	quackplayed=true
 else 
 	plr.quacking = false
 	quackplayed=false
 end    
 
 --check col up and down
 if plr.dy>0 then
 	plr.falling=true
 	plr.landed=false
 	plr.jumping=false
 	
 	plr.dy=limit_speed(plr.dy,plr.max_dy)
 
 --check top down col	
 if collide_map(plr,"down",0) and not btn(‚¨áÔ∏è) then
 		plr.landed = true
 		plr.falling=false
 		plr.dy=0
 		plr.y-=((plr.y+plr.h+1)%8)-1
 elseif collide_map(plr,"down",0) and btn(‚¨áÔ∏è) then
			sfx(2)
 elseif collide_map(plr,"down",1) then
 		plr.landed = true
 		plr.falling=false
 		plr.dy=0
 		plr.y-=((plr.y+plr.h+1)%8)-1
	end
 elseif plr.dy<0 then
 	plr.jumping=true
 	if collide_map(plr,"up",1) then
 		plr.dy=0
 	end
 end
 
  
 plr.x+=plr.dx
 plr.y+=plr.dy
 	
 	--wrap
 if plr.x > 127 then plr.x = 0
 elseif plr.x < 0 then plr.x = 127 end
end

function player_animate()
	--quacking
	local quack=0
	if plr.quacking then quack = 16  end
	
	--jump/fall
	if plr.jumping then
		plr.sp = 3+quack
	
	 	
	elseif plr.falling then
		plr.sp = 4+quack

	
	--running
	elseif plr.running then
		if plr.sp > run_end+quack then plr.sp = run_strt+quack 
		elseif plr.sp < run_strt+quack then plr.sp = run_strt+quack
		end
		if time()-plr.anim>0.01 then
			plr.anim = time()
			plr.sp+=1
			if plr.sp>6+quack then
				plr.sp=1+quack
			end
		end
		
		
		--idle
	else
	  if plr.sp < idle_strt+quack then plr.sp = idle_strt+quack end
			if time()-plr.anim>.1 then
				plr.anim=time()
				plr.sp+=1
				if plr.sp>38+quack then
					plr.sp=33+quack
				end
			end
	end
end

--limit speed
function limit_speed(num,maximum)
  return mid(-maximum,num,maximum)
end
