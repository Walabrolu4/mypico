--guns
function add_gun(sx,sy,sdx,sdy,gsp)
	add(guns,{
	x=sx,
	y=sy,
	w=8,
	h=8,
	flp=false,
	dx=sdx,
	dy=sdy,
	max_dx=2,
	max_dy=3,
	sp = gsp,
	
	draw = function(s)
		spr(s.sp,s.x,s.y,1,1,s.flp)
	end,
	
	update = function(s)
			--physics
			s.dy+=gravity
			s.dx*=friction
			
			--check top down col	
		 if collide_map(s,"down",0) then
		 	s.dy=0
		 	s.y-=((s.y+s.h+1)%8)-1
		 elseif collide_map(s,"down",1) then
		 		s.dy=0
		 		s.y-=((s.y+s.h+1)%8)-1
			end
			
			--update it
			s.x+=limit_speed(s.dx,s.max_dx)
 		s.y+=limit_speed(s.dy,s.max_dy)
	end
	
	})
end

