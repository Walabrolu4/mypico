--draw
function _draw()
    cls(0)
				map(0,0) 
				
				for i,g in ipairs(guns) do
					--draw_box(g)
					g:draw()
				end
				
				
				
    spr(plr.sp, plr.x, plr.y,1,1,plr.flp)
    --draw_box(plr)

    if plr.gun_sp != -1 then
    	if not plr.flp then spr(plr.gun_spr,plr.x+gun_offset,plr.y+1,1,1,plr.flp) else
    	spr(plr.gun_spr,plr.x-gun_offset,plr.y+1,1,1,plr.flp) end
    end
    --spr_r(7,48,40,sin(time()*0.2)*50,2,2)
    if trophy_show then
    	spr_r(37,plr.x-4,plr.y-18,sin(time()*0.2)*20,2,2)
    end
end

function draw_box(obj)
	line(obj.x,obj.y,obj.x+obj.w,obj.y,5) -- top
	line(obj.x,obj.y+obj.h,obj.x+obj.w,obj.y+obj.h,5) -- bottom
	line(obj.x,obj.y,obj.x,obj.y+obj.h,5) -- left
	line(obj.x+obj.w,obj.y,obj.x+obj.w,obj.y+obj.h,5) -- right
end
